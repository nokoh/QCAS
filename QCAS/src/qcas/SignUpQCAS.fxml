<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="qcas.SignUpQCASController">
    <children>
      <ImageView fitHeight="550.0" fitWidth="790.0" layoutX="-1.0" pickOnBounds="true">
         <image>
            <Image url="@../CMU%20Plaid.jpg" />
         </image>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="475.0" layoutX="21.0" layoutY="60.0" stroke="BLACK" strokeType="INSIDE" width="748.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" height="84.0" layoutX="21.0" layoutY="14.0" stroke="BLACK" strokeType="INSIDE" width="748.0">
         <fill>
            <LinearGradient endX="1.0" endY="1.0" startX="1.0" startY="0.20952380952380953">
               <stops>
                  <Stop color="#801212" />
                  <Stop color="#400b0b" offset="1.0" />
               </stops>
            </LinearGradient>
         </fill></Rectangle>
      <Button id="returnHomeButton" fx:id="returnHomeButton" layoutX="61.0" layoutY="285.0" mnemonicParsing="false" onAction="#returnHomeButtonClicked" prefHeight="25.0" prefWidth="136.0" style="-fx-background-color: maroon;" text="Return Home" textFill="WHITE">
         <effect>
            <InnerShadow />
         </effect></Button>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="230.0" layoutY="19.0" prefHeight="75.0" prefWidth="306.0" text="New User - Sign Up" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Segoe UI Light" size="36.0" />
         </font>
      </Label>
      <VBox layoutX="255.0" layoutY="139.0" prefHeight="318.0" prefWidth="256.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <HBox prefHeight="84.0" prefWidth="253.0">
                     <children>
                        <VBox alignment="CENTER" prefHeight="84.0" prefWidth="253.0">
                           <children>
                              <Label text="First Name:" />
                              <TextField fx:id="firstNameField" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="76.0" prefWidth="256.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="69.0" prefWidth="253.0">
                     <children>
                        <Label text="Last Name:" />
                        <TextField fx:id="lastNameField" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="84.0" prefWidth="253.0">
                     <children>
                        <Label text="ID Number:" />
                        <TextField fx:id="idNumberField" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <VBox alignment="CENTER" prefHeight="64.0" prefWidth="256.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="34.0" prefWidth="256.0" text="Password:" />
                  <PasswordField fx:id="passwordField" />
               </children>
            </VBox>
            <HBox alignment="CENTER" prefHeight="46.0" prefWidth="256.0" />
            <VBox alignment="CENTER" prefHeight="64.0" prefWidth="256.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="34.0" prefWidth="256.0" text="Staff Role" />
                  <TextField fx:id="staffRole" />
               </children>
            </VBox>
         </children>
      </VBox>
      <Button fx:id="signUpButton" layoutX="582.0" layoutY="285.0" mnemonicParsing="false" onAction="#signUp" prefHeight="25.0" prefWidth="136.0" style="-fx-background-color: maroon;" text="Sign Up" textFill="WHITE">
         <effect>
            <InnerShadow />
         </effect></Button>
      <Text fx:id="errorMessageLabel" layoutX="241.0" layoutY="502.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="306.0">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Text>
      <Label alignment="CENTER" layoutX="194.0" layoutY="113.0" prefHeight="17.0" prefWidth="378.0" text="Please Enter Your Credentials Above, Then Click 'Sign Up'">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
   </children>
   <effect>
      <InnerShadow />
   </effect>
</AnchorPane>
